local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "Bobsled Ride Down a Mountainside",
    LoadingTitle = "Bobsled Ride Down a Mountainside",
    LoadingSubtitle = "by 파"
})

Rayfield:Notify({
    Title = "로블록스 최고의 커뮤니티",
    Content = "https://discord.gg/localx",
    Duration = 30
})

local Tab = Window:CreateTab("Main", 4483362458)

local Section1 = Tab:CreateSection("Troll")

Tab:CreateButton({
    Name = "Return All To Spawn",
    Callback = function()
        for _, plr in game.Players:GetPlayers() do
            if plr ~= game.Players.LocalPlayer and plr.Character then
                game.ReplicatedStorage.Remotes.ReturnToSpawn:FireServer(plr.Character)
            end
        end
    end
})
